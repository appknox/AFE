#!/system/bin/sh
 
#remove old files
rm /system/bin/busybox > /dev/null 2>&1
rm /data/local/root.sh > /dev/null 2>&1
rm /data/dalvik-cache/system@app@Superuser.apk@classes.dex > /dev/null 2>&1
